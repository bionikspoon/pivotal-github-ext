{
  "name": "Pivotal Github Integration",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension adds pivotal information to Github",
  "homepage_url": "https://manuphatak.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": ["https://github.com/*"],
  "content_security_policy":
    "script-src 'self'; object-src 'self'; connect-src 'self' https://localhost:8080",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["inject.js"]
    }
  ]
}
